{
    "name": "laravel-auth-app",
    "description": "A Laravel application for user authentication using PostgreSQL.",
    "type": "project",
    "require": {
        "php": "^7.3|^8.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0",
        "laravel/framework": "^8.0",
        "laravel/tinker": "^2.0",
        "laravel/passport": "^10.0",
        "doctrine/dbal": "^2.10",
        "laravel/sanctum": "^2.0"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "mockery/mockery": "^1.4",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "php artisan package:discover --ansi"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}